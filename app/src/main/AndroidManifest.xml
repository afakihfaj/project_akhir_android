<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidmypos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.InItemActivity"></activity>
        <activity android:name=".Activity.InputUser"></activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Activity.ReadUserActivity" />
        <activity android:name=".Activity.ReadSupplierActivity" />
        <activity android:name=".Activity.InSupplierActivity" />
        <activity android:name=".Activity.ReadUnitActivity" />
        <activity android:name=".Activity.InUnitActivity" />
        <activity android:name=".Activity.ReadItemActivity" />
        <activity android:name=".Activity.ReadCategoryActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".Activity.InCategoryActivity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>